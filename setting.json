{
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Ayu Dark Bordered",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "MikeBovenlander.formate"
  },
  "bracket-pair-colorizer-2.depreciation-notice": false,
  "editor.formatOnSave": true,
  "settingsSync.ignoredSettings": [
    "editor.experimental.stickyScroll.enabled"
  ],
  "[twig]": {
    "editor.defaultFormatter": "mblode.twig-language-2"
  },
  "editor.stickyScroll.enabled": true,
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "workbench.editorAssociations": {
    "*.docx": "default"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  // "editor.showUnused": true,
  // "workbench.colorCustomizations": {
  //   "editorUnnecessaryCode.border": "#dd7aab",
  //   "editorUnnecessaryCode.foreground": "#ff0000"
  // },
  "files.autoSave": "afterDelay",
  "tabnine.experimentalAutoImports": true,
  "explorer.confirmDelete": false,
  "hediet.vscode-drawio.resizeImages": null,
  "scss.format.enable": false
}
